package ejemplo03_Encapsulamiento;

import ejemplo02_ClasesYObjetos.Libro;

public class Ejemplo
{
	public static void main(String[] args)
	{		
		Libro libro1 = new Libro();

		libro1.setTitulo("El juego de Ender");
		libro1.setEditorial("Ediciones B");
		libro1.setNumPaginas(-5);

		libro1.mostrarInformacion();
		
		libro1.setNumPaginas(260);
		System.out.println("El libro tiene " + libro1.getNumPaginas() + " pags.");
	}
}
